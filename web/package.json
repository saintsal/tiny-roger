{
	"name": "jolly-roger-web",
	"version": "0.0.1",
	"description": "Frontend for jolly-roger.",
	"private": true,
	"devDependencies": {
		"@sveltejs/adapter-static": "^2.0.1",
		"@sveltejs/kit": "^1.5.0",
		"@tailwindcss/forms": "^0.5.3",
		"@tailwindcss/typography": "^0.5.9",
		"autoprefixer": "^10.4.7",
		"daisyui": "^2.18.1",
		"ipfs-gateway-emulator": "4.2.1-ipfs.2",
		"postcss": "^8.4.14",
		"postcss-load-config": "^4.0.1",
		"prepare-for-web": "^0.2.0",
		"prettier": "^2.8.4",
		"prettier-plugin-svelte": "^2.8.1",
		"svelte": "^3.54.0",
		"svelte-check": "^3.0.1",
		"svelte-preprocess": "^4.10.7",
		"sveltejs-adapter-ipfs": "^0.4.8",
		"tailwindcss": "^3.1.5",
		"tslib": "^2.4.1",
		"typescript": "^4.9.5",
		"vite": "^4.0.0"
	},
	"type": "module",
	"scripts": {
		"prepare": "prepare-for-web --config src/web-config.json --target static --targetFile src/app.html",
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check .",
		"format": "prettier --plugin-search-dir . --write .",
		"serve": "ipfs-emulator --only -d build -p 8080"
	},
	"dependencies": {
		"named-logs": "^0.2.0",
		"theme-change": "^2.3.0"
	}
}
